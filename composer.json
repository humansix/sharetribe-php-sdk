{
  "name": "humansix/sharetribe-php-sdk",
  "description": "Sharetribe SDK for PHP",
  "license": "OSL-3.0",
  "minimum-stability": "stable",
  "authors": [
  	{
      "name": "Ludovic Drin",
      "homepage": "https://www.humansix.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sharetribe\\Sdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sharetribe\\Sdk\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "psr/http-message": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-message-implementation": "^1.0",
    "php-http/discovery": "^1.6"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "phpunit/phpunit": "^9.0",
    "donatj/mock-webserver": "^2.0",
    "http-interop/http-factory-guzzle": "^1.0",
    "php-http/guzzle6-adapter": "^2.0"
  },
  "config": {
    "bin-dir": "bin",
    "platform": {
      "php": "7.3.11"
    }
  }
}
